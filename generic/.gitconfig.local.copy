[credential]
	helper = cache --timeout=3600
	# helper = osxkeychain

[url "git@github.com:"]
	insteadOf = https://github.com/

[alias]
	ls = log --oneline
	br = branch -vv --sort=-committerdate
	st = status
	pik = cherry-pick -x
	co = checkout
	mdiff = diff master
	shush = update-index --skip-worktree
	unshush = update-index --no-skip-worktree
	tags = show-ref --tags --abbrev
